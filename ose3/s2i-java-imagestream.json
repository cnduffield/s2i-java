{
   "kind": "ImageStream",
   "apiVersion": "image.openshift.io/v1",
   "metadata": {
      "name": "s2i-java-gradle-v1"
      "namespace": "openshift"
   },
   "spec": {
      "tags": [
         {
            "name": "latest",
            "annotations": {
               "description": "Java Gradle OpenJDK S2I image",
               "iconClass": "icon-jboss",
               "tags": "builder,java,maven,gradle,springboot,vertx,widlfly-swarm,dropwizard,microservices,fatjar",
               "supports": "java:8,maven:3,gradle:2.6",
               "sampleRepo": "https://github.com/cnduffield/gradlespring.git",
               "version": "latest"
            },
            "from": {
               "kind": "DockerImage",
               "name": "docker.io/cnduffield/s2i-gradle:latest"
            }
         }
      ]
   }
}
